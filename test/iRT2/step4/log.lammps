fix  totWA all wallforce 1 1 ${zw} ${delta} ${rw} ${clwall1} file  fcc111-T1-walls.lmp
fix  totWA all wallforce 1 1 0.62 ${delta} ${rw} ${clwall1} file  fcc111-T1-walls.lmp
fix  totWA all wallforce 1 1 0.62 0.25 ${rw} ${clwall1} file  fcc111-T1-walls.lmp
fix  totWA all wallforce 1 1 0.62 0.25 1.12246204830937 ${clwall1} file  fcc111-T1-walls.lmp
fix  totWA all wallforce 1 1 0.62 0.25 1.12246204830937 8.0998403805961 file  fcc111-T1-walls.lmp
fix  totWB all wallforce ${eps} ${sigma} ${zw} ${delta} ${rw} ${clwall2} file  fcc111-T1-walls.lmp
fix  totWB all wallforce 1 ${sigma} ${zw} ${delta} ${rw} ${clwall2} file  fcc111-T1-walls.lmp
fix  totWB all wallforce 1 1 ${zw} ${delta} ${rw} ${clwall2} file  fcc111-T1-walls.lmp
