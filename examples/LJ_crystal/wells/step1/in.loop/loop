label		Lab0
variable	LoopIndx  loop ${nloop}



variable    lam equal ${lambda}+${delam}
variable    lambda equal ${lam}

fix f2 all wellPforce ${dw} ${rw} ${expp} ${lambda} file wells.lmp

print       "Well depth ${lambda}"
run   ${eqnts}
variable    cnt equal ${cntt}+1
variable    cntt equal ${cnt}
fix  f5 all ave/time 1 100 100 c_thermo_temp c_thermo_pe f_f2 v_lambda file  out/ave.F.${cnt}.out

run  ${nts}

unfix f5
unfix f2

write_data data/Fstep1.${lambda}.data nocoeff
next		LoopIndx

if "${lambda} >= ${uplimit}" then "jump SELF break" else " jump SELF Lab0 "
label		break
variable	LoopIndx delete

